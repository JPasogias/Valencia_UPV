
#newTeam.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
  .modal-dialog.modal-md
    .modal-content
      // Modal Header
      .modal-header
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') Ã—
        h4#myModalLabel.modal-title New team
      // Modal Body
      .modal-body
        .row
          .col-md-6
            label Name of the team
            input.form-control.typeahead.tt-input(
              type="text" 
              name="src" 
              placeholder="Name of the team" 
              autocomplete="off" 
              spellcheck="false" 
              dir="auto" 
              style="position: relative; vertical-align: top; background-color: transparent;"
              ng-model="team.name"
              )
          .col-md-6
            br
            label.switch.switch-round
              span Is private group?
              span(style="padding-left:20px")
              input(id="isPrivate" type="checkbox" ng-model="isPrivate" checked="")
              span.switch-label(for="isPrivate" data-on="YES" data-off="NO")
        .row
          br
          .col-md-12
            label Description
            .fancy-form
              textarea.form-control.word-count(
                rows='4',
                data-maxlength='200',
                data-info='textarea-words-info',
                placeholder="Destined members..."
                ng-model="team.description" 
                )
              i.fa.fa-comments
              span.fancy-hint.size-11.text-muted
                strong Track:
                |  200 Words allowed!
                span.pull-right
                  span {{team.description.length}}/200
                  |  Words
        .row
          br
          .col-md-12
            #panel-misc-portlet-l2.panel.panel-default
              .panel-heading
                span.elipsis
                  strong Select a badge
                ul.nav.nav-tabs.pull-right
                  li.active
                    a(href='#ttab1_nobg', data-toggle='tab')
                      | PAGE 1 
                  li
                    a(href='#ttab2_nobg', data-toggle='tab')
                      | PAGE 2 
              .panel-body
                .tab-content
                  #ttab1_nobg.tab-pane.active
